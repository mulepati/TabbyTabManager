{
    "name" : "Tabby the tab manager",
    "description" : "Tab manager with voice assistant.",
    "version" : "0.1.0",
    "manifest_version" : 2,
    "icons" : {
        "16" : "./16x16-2.png",
        "32" : "./32x32.png",
        "48" : "./48x48.png",
        "128" : "./128x128.png"
    },
    "background" : {
        "scripts" : ["./background.js"]
    },

    "options_page" : "./options.html",
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "tabs",
        "https://www.google.com/*"
    ]
}